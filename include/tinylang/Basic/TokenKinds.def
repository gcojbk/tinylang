#ifndef TOK
#define TOK(ID)
#endif

#ifndef PUNCTUATOR
#define PUNCTUATOR(ID, SP) TOK(ID)
#endif

#ifndef KEYWORD
#define KEYWORD(ID, FLAG) TOK(kw_##ID)
#endif


TOK(unknown)
TOK(eof)
TOK(identifier)
TOK(integer_literal)

PUNCTUATOR(plus, "+")
PUNCTUATOR(minus, "-")

KEYWORD(BEGIN, KEYALL)
KEYWORD(CONST, KEYALL)


#undef TOK
#undef PUNCTUATOR
#undef KEYWORD
